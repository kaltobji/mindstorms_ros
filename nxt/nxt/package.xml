<package>
  <name>nxt</name>
  <version>1.0.0</version>
  <description>This  has basic interfaces for interacting with ROS and NXT. The software in this
     is intended to run on top of the default NXT firmware provided by lego.</description>
  <maintainer email="tfoote@willowgarage.com">Tully Foote</maintainer>
  <maintainer email="meeussen@willowgarage.com">Wim Meeussen</maintainer>
  <maintainer email="mwise@willowgarage.com">Melonee Wise</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/nxt</url>
  <!-- <url type="bugtracker"></url> -->

  <author>Tully Foote</author>
  <author>Wim Meeussen</author>
  <author>Melonee Wise</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->


  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>nxt_controllers</run_depend>
  <run_depend>nxt_description</run_depend>
  <run_depend>nxt_lxf2urdf</run_depend>
  <run_depend>nxt_msgs</run_depend>
  <run_depend>nxt_python</run_depend>
  <run_depend>nxt_ros</run_depend>

  <!-- Dependencies needed only for running tests. -->


</package>
